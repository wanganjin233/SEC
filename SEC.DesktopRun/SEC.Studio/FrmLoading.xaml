<Window x:Class="SEC.Studio.FrmLoading"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SEC.Studio"
        mc:Ignorable="d"
        Height="450" Width="800" WindowStartupLocation="CenterScreen" WindowStyle="None">
    <Window.Resources>
        <ImageBrush x:Key="freeMachineImageBrush" ImageSource="319fe68a3178c63c762f9921e6f12c56.png"/>
    </Window.Resources>
    <Canvas Name="myCanvas" Focusable="True">
        <Rectangle Name="background" Height="400" Width="1262"/>
        <Rectangle Name="background2" Height="400" Width="1262" Canvas.Left="1262" />
        <Rectangle Fill="{StaticResource freeMachineImageBrush}" Height="48" Width="128" Canvas.Left="336"/>
    </Canvas>
</Window>
